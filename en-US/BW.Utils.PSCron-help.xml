          <dev:type>
            <maml:name>DateTime</maml:name>
            <maml:uri />
          </dev:type>
          <dev:defaultValue>( Get-PSCronTimestamp -Resolution Day ).AddDays( 1 )</dev:defaultValue>
        </command:parameter>
